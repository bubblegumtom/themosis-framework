    "fzaninotto/faker": "Required to use the eloquent factory builder (^1.8)."
  },
  "provide": {
    "psr/container-implementation": "1.0"
  },
  "conflict": {
    "tightenco/collect": "<5.5.33"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
